{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {

    "sitemap":{
      "baseUrl": "https://docs.emgage.com",
      "priority": 0.1,
      "changefreq": "monthly"
    },

    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**", "articles/images/**", "articles/resources/**.*"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    
    "globalMetadata": {
      "_appTitle": "Emgage",
      "_enableSearch": true,
      "_appFooter": "Â© Copyright Emgage Inc.",
      "_appLogoPath": "./articles/images/logo.png",
      "_gitContribute": {
        "repo":"https://github.com/emgage/emgage-docs",
        "branch":"master"
      }

    },

    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
    
  }
}